local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local twitterCodes = playerGui:WaitForChild("TwitterCodes", 5)
if twitterCodes then
    if twitterCodes:IsA("Frame") then
        twitterCodes.Visible = true
        twitterCodes.Position = UDim2.new(0.5, 0, 0.5, 0)
        twitterCodes.Size = UDim2.new(0, 300, 0, 200)
        twitterCodes.ZIndex = 10
    end
else
    return
end

for _, kod in pairs(kodlar) do
    local frame = twitterCodes:FindFirstChild("Frame")
    if frame then
        local textBox = frame:FindFirstChild("TextBox")
        if textBox and textBox:IsA("TextBox") then
            textBox.Text = kod
        else
            return
        end
    else
        return
    end

    local confirmButton = frame:FindFirstChild("ConfirmButton")
    if confirmButton then
        if confirmButton:IsA("ImageButton") then
        end
    else
        return
    end

    wait(1)
    local invalidText = frame:FindFirstChild("InvalidTextShadow") or frame:FindFirstChild("RealText")
    if invalidText and (invalidText:IsA("TextLabel") or invalidText:IsA("TextBox")) then
        if string.find(string.lower(invalidText.Text), "nil") then
            print("Yanlış")
        else
            print("Doğru")
        end
    else
        return
    end
end
